@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

type:challenge a rdfs:Class ;
    rdfs:label "challenge" ;
    property:description "An innovation challenge is a means to encourage creation and elaboration of the best and most sustainable ideas from the best innovators by providing a competition for innovation grants." ;
    dc:relation <https://en.wikipedia.org/wiki/Innovation_competition> .

type:source a rdfs:Class ;
    rdfs:label "source" ;
    property:description "An identifier for a source of innovation challenges" ;
    dc:relation <https://en.wikipedia.org/wiki/Source_text> .

type:topic a rdfs:Class ;
    rdfs:label "topic" ;
    property:description "A subject or classification of an innovation opportunity derived using topic classification" ;
    dc:relation <https://en.wikipedia.org/wiki/Topic_and_comment> .

type:person a rdfs:Class ;
    rdfs:label "person" ;
    property:description "A person" ;
    dc:relation <https://en.wikipedia.org/wiki/Person> .

type:occupation a rdfs:Class ;
    rdfs:label "occupation" ;
    property:description "An occupation" ;
    dc:relation <https://en.wikipedia.org/wiki/Occupation> .

type:region a rdfs:Class ;
    rdfs:label "region" ;
    property:description "A region" ;
    dc:relation <https://en.wikipedia.org/wiki/Region> .

type:dept a rdfs:Class ;
    rdfs:label "department" ;
    property:description "A department" ;
    dc:relation <https://en.wikipedia.org/wiki/Department> .

type:unit a rdfs:Class ;
    rdfs:label "unit" ;
    property:description "A unit" ;
    dc:relation <https://en.wikipedia.org/wiki/Unit> .

type:grade a rdfs:Class ;
    rdfs:label "grade" ;
    property:description "A grade" ;
    dc:relation <https://en.wikipedia.org/wiki/Grade> .

type:organisation a rdfs:Class ;
    rdfs:label "organisation" ;
    property:description "An organisation" ;
    dc:relation <https://en.wikipedia.org/wiki/Organisation> .

property:closes a rdfs:Property ;
    rdfs:label "closes" ;
    property:description "The closing date of an innovation challenge" .

property:description a rdfs:Property ;
    rdfs:label "description" ;
    property:description "A full elaborative explanation" .

property:has-source a rdfs:Property ;
    rdfs:label "has-source" .

property:has-topic a rdfs:Property ;
    rdfs:label "has-topic" ;
    property:description "Relationship describing innovation topics",
        "Relationship describing sources of innovation challenges" .

property:opens a rdfs:Property ;
    rdfs:label "opens" ;
    property:description "The opening date of an innovation challenge" .

property:title a rdfs:Property ;
    rdfs:label "title" ;
    property:description "A descriptive summary title" .

dc:relation a rdfs:Property ;
    rdfs:label "relation" ;
    property:description "Link to original source material" .

property:founded a rdfs:Property ;
    rdfs:label "founded" ;
    property:description "Organisation founding year" .

property:name a rdfs:Property ;
    rdfs:label "name" ;
    property:description "Person's name" .

property:grade a rdfs:Property ;
    rdfs:label "grade" ;
    property:description "Job grade or rank" .

property:team a rdfs:Property ;
    rdfs:label "team" ;
    property:description "Name of team" .

property:department a rdfs:Property ;
    rdfs:label "department" ;
    property:description "Name of department" .

property:unit a rdfs:Property ;
    rdfs:label "unit" ;
    property:description "Name of organisational unit" .

property:organisation a rdfs:Property ;
    rdfs:label "organisation" ;
    property:description "Name of organisational" .

property:fte a rdfs:Property ;
    rdfs:label "fte" ;
    property:description "Full-time employee count" .

property:occupation a rdfs:Property ;
    rdfs:label "occupation" ;
    property:description "Occupation" .

property:region a rdfs:Property ;
