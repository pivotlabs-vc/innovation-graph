{
    "id": "organisations",
    "description": "UK government organisations with innovation remit",
    "contributors": [
	"cybermaggedon"
    ],
    "origin": "Copied from Wikipedia, and the organisations own websites",
    "copyright": "Not defined, information in the public domain",
    "processing": "csv",
    "input": "orgs.csv",

    "fields": [
	"id",
	"description",
	"web-url",
	"label",
	"founded"
    ],

    "object": {

	"class": "type:sponsor",
	"id-prefix": "sponsorinst:",
	"id-field": "id",

	"properties": [
	    {
		"predicate": "rdfs:comment",
		"object-field": "description"
	    },
	    {
		"predicate": "rdfs:seeAlso",
		"object-field": "web-url"
	    },
	    {
		"predicate": "rdfs:label",
		"object-field": "label"
	    },
	    {
		"predicate": "organisation:founded",
		"object-field": "founded",
		"datatype": "xsd:gYear"
	    }
	]

    }

}

