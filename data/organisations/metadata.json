{
    "id": "organisations",
    "description": "UK government organisations with innovation remit",
    "contributors": [
	"cybermaggedon"
    ],
    "origin": "Copied from Wikipedia, and the organisations own websites",
    "copyright": "Not defined, information in the public domain",
    "processing": "csv",
    "input": "orgs.csv",

    "fields": [
	"id",
	"description",
	"web-url",
	"label",
	"founded"
    ],

    "objects": [
	{
	    "class": "schema:Organization",
	    "id-prefix": "organisationinst:",
	    "id-fields": ["id"],
	    "with-id": "derive",
	    "properties": [
		{
		    "predicate": "rdfs:comment",
		    "field": "description"
		},
		{
		    "predicate": "rdfs:seeAlso",
		    "field": "web-url"
		},
		{
		    "predicate": "rdfs:label",
		    "field": "label"
		},
		{
		    "predicate": "organisation:founded",
		    "field": "founded",
		    "datatype": "xsd:gYear"
		}
	    ]
	}
    ]

}

