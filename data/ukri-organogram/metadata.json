{
    "id": "ukri-organogram",
    "description": "UKRI Organograms",
    "contributors": [
	"cybermaggedon"
    ],
    "origin": "data.gov.uk website",
    "copyright": "Open Government Licence",
    "licence": "https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/",
    "processing": "csv",

    "input": "organogram-senior.csv",
    "skip": 0,
    "limit": 1,

    "fields-from-header": true,

    "objects": [
	{
	    "class": "schema:Person",
	    "id-prefix": "personinst:ukri/",
	    "id-fields": ["Post Unique Reference"],
	    "with-id": "hash",
	    "properties": [
		{
		    "predicate": "schema:name",
		    "field": "Name"
		},
		{
		    "predicate": "schema:jobTitle",
		    "field": "Job Title"
		},
		{
		    "class": "schema:Organization",
		    "predicate": "schema:affiliation",
		    "id-prefix": "organisationinst:",
		    "id-fields": ["Organisation"],
		    "with-id": "derive",
		    "properties": [
			{
			    "class": "schema:Organization",
			    "predicate": "schema:worksFor",
			    "id-fields": ["Parent Department"],
			    "with-id": "map",
			    "properties": [
			    ],
			    "map": {
				"Department for Business, Energy & Industrial Strategy": "departmentinst:beis"
			    }
			}
		    ]
		},
		{
		    "class": "schema:Occupation",
		    "predicate": "schema:hasOccupation",
		    "id-prefix": "http://pivotlabs.vc/innov/occupation/ukri/",
		    "id-fields": ["Post Unique Reference"],
		    "with-id": "derive",
		    "properties": [
			{
			    "predicate": "schema:name",
			    "field": "Job Title"
			},{
			    "predicate": "schema:responsibiltiies",
			    "field": "Job/Team Function"
			}
		    ]
		}
	    ]
	}
    ]
}

